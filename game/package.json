{
  "name": "last-fm-on-this-day",
  "version": "1.0.0",
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
